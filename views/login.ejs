<body>
    <header>
      <div id="logo"></div>
    </header>
    <div class = "panel panel-default" style="">
        <div class = "panel-heading">
            <h4 class = "panel-title">Sign In</h4>
        </div>
        <div class = "panel-body">
            <form class ="panel-form" role = "form" method = "POST" action="/login/login">
                <filedset>
                    <div class = "form-group">
                        <input class ="form-control form-username" placeholder = "Username" name = "username" type = "text">
                    </div>
                    <div class = "form-group">
                        <input class = "form-control form-pass" placeholder = "Password" name = "password" type = "password" value = "">
                    </div>
                    <div class = "form-button">
                        <input class = "btn btn-lg btn-success btn-block" type = "submit" value = "Sign In">
                    </div>
                </fieldset>
            </form>
        </div>
        <div class = "panel-footing">
        </div>
    </div>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
