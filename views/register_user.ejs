<% include _header %>

    <div class = "panel panel-default" style="">
        <div class = "panel-heading">
            <h4 class = "panel-title">Sign Up</h4>
        </div>
        <div class = "panel-body">
            <form class ="panel-form" role = "form" method = "POST" action="/register_user">
                <filedset>
                    <div class = "form-group">
                        <input class ="form-control form-username" placeholder = "Username" name = "username" type = "text">
                    </div>
                    <div class = "form-group">
                        <input class = "form-control form-pass" placeholder = "Password" name = "password" type = "password" value = "">
                    </div>
                    <div class = "form-button">
                        <input class = "btn btn-lg btn-success btn-block" type = "submit" value = "Register">
                    </div>
                </fieldset>
            </form>
        </div>
    </div>

<% include _footer %>
