<% include _header %>
<h2>freetime一覧</h2>
<% for (var item of freetimes) { %>
<p>
<%= ["日", "月", "火", "水", "木", "金", "土"][item.day] %>曜日
<%= Math.floor(item.timecode_from / 60) %>時
<%= item.timecode_from % 60 %>分
から
<%= Math.floor(item.timecode_to / 60) %>時
<%= item.timecode_to % 60 %>分
</p>
<% } %>
<h1>freetime編集</h1>
<form name="day" action="/user_edit" method="post">
	<select name="day">
		<option value="0">日</option>
		<option value="1">月</option>
		<option value="2">火</option>
		<option value="3">水</option>
		<option value="4">木</option>
		<option value="5">金</option>
		<option value="6">土</option>
	</select>
	曜日
	<input name="from_h" type="number" min="0" max="23">
	時
	<input name="from_m" type="number" min="0" max="59">
	分
	から
	<input name="to_h" type="number" min="0" max="23">
	時
	<input name="to_m" type="number" min="0" max="59">
	分
	<input type="submit" value="追加"> 
</form>
<% include _footer %>
